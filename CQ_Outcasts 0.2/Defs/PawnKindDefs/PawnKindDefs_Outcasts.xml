<?xml version="1.0" encoding="utf-8" ?>

<Defs>

  <PawnKindDef Name="Outcasts_Base" Abstract="True">
	<race>Human</race>
    <combatPower>45</combatPower>
    <initialWillRange>1~5</initialWillRange>
    <initialResistanceRange>1~20</initialResistanceRange>
  </PawnKindDef>

  <PawnKindDef ParentName="Outcasts_Base">
    <defName>Outcasts_ZDreamer</defName>
    <label>obsessed builder</label>
	<backstoryFiltersOverride>
	    <li>
            <categoriesChildhood>
            <li>Outcasts_ZDreamer_Childhood</li>
            </categoriesChildhood>
            <categoriesAdulthood>
            <li>Outcasts_ZDreamer_Adulthood</li>
            </categoriesAdulthood>
        </li>
    </backstoryFiltersOverride>
    <combatPower>45</combatPower>
    <itemQuality>Poor</itemQuality>
    <minGenerationAge>30</minGenerationAge>
	<maxGenerationAge>50</maxGenerationAge>
    <isFighter>false</isFighter>
    <gearHealthRange>0.8~2.0</gearHealthRange>
	<apparelTags>
        <li>IndustrialBasic</li>
    </apparelTags>
    <weaponTags>
	    <li>NeolithicMeleeDestructive</li>
    </weaponTags>
    <weaponMoney>900~1000</weaponMoney>
    <apparelMoney>500~1000</apparelMoney>
    <apparelAllowHeadgearChance>0.4</apparelAllowHeadgearChance>
	<requiredWorkTags>
        <li>Violent</li>
		<li>ManualDumb</li>
		<li>ManualSkilled</li>
    </requiredWorkTags>
	<disallowedTraits>		
		<li>Cannibal</li>
		<li>Wimp</li>
		<li>Gourmand</li>
    </disallowedTraits>
    <disallowedTraitsWithDegree>
        <Industriousness>-2</Industriousness>
    </disallowedTraitsWithDegree>
  </PawnKindDef>

